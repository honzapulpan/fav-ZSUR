    "import numpy as np\n",
    "\n",
    "def dist_m(data):\n",
    "    \n",
    "    dl = len(data)\n",
    "    dm = np.zeros((dl,dl))\n",
    "\n",
    "    for i in range(0,dl):\n",
    "       dm[i,:] = np.sum((data-data[i,:])**2, axis=1)\n",
    "\n",
    "    np.fill_diagonal(dm, np.nan)\n",
    "    \n",
    "    return dm"
   ]
